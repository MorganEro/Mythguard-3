
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model TestProduct {
  id        String   @id @default(uuid())
  name      String
  price     Int
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}